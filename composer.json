{
  "name": "malvik-lab/ip-api-client",
  "description": "A PHP library for interfacing with ip-api.com, providing geolocation details based on an IP address.",
  "keywords": ["ip", "address", "ip-api", "geolocation", "geoip", "location", "internet", "api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Vittorio Malaspina",
      "email": "vittorio.malaspina@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "MalvikLab\\IpApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.0",
    "rakit/validation": "^1.4",
    "cuyz/valinor": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "minimum-stability": "stable"
}
